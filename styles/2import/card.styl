.card.card-beautiful
  box-shadow 0px 4px 9px rgba(51, 95, 226, 0.3)
  border-radius 8px
  border none

.card.card-detail
  background #FFFFFF
  box-shadow 0px 3px 12px rgba(86, 16, 20, 0.18)
  border-radius 4px
  border none

.card.card-beautiful.card-beautiful--lesser
  box-shadow 0px 4px 9px rgba(51, 95, 226, 0.15)

.card.card-blue-royal
  background-color blue-royal

.card.card-flat
  box-shadow none
  border-radius 0
  border none

.card-beauty
  box-shadow 0px 4px 4px rgba(0, 0, 0, 0.25)
  border 1px solid #DDDDDD
  background #FFFFFF
  border-radius 10px
  align-items center
  justify-content center
